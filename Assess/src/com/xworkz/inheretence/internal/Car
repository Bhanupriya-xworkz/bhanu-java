package com.xworkz.inheretence.internal;

    public class Car extends Vehicle {

        public Car() {
            super();
            System.out.println("Car constructor");
        }

        public void startEngine() {
            super.startEngine();
            System.out.println("Starting the car engine...");
        }

        public void stopEngine() {
            super.stopEngine();
            System.out.println("Stopping the car engine...");
        }

        public void refuel() {
            super.refuel();
            System.out.println("Refueling the car...");
        }

        public void checkTires() {
            super.checkTires();
            System.out.println("Checking the car tires...");
        }

        public void honk() {
            super.honk();
            System.out.println("Honking the car horn...");
        }

        public void openTrunk() {
            System.out.println("Opening the car trunk...");
        }

        public void turnOnAC() {
            System.out.println("Turning on the car air conditioning...");
        }

        public void checkFuelLevel() {
            System.out.println("Checking the fuel level in the car...");
        }
    }

